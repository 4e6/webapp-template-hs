                     , text                    == 1.2.2.2
                     , th-lift                 == 0.7.7
                     , th-lift-instances       == 0.1.11
                     , time                    == 1.8.0.2
                     , time-locale-compat      == 0.1.1.3
                     , transformers            == 0.5.2.0
                     , transformers-base       == 0.4.4
                     , transformers-compat     == 0.5.1.4
                     , type-fun                == 0.1.1
                     , unix                    == 2.7.2.2
                     , unix-compat             == 0.4.3.1
                     , unix-time               == 0.3.7
                     , unordered-containers    == 0.2.8.0
                     , uri-bytestring          == 0.2.3.3
                     , utf8-string             == 1.0.1.1
