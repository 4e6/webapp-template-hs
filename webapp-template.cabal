  default-extensions:  ConstraintKinds
                     , DataKinds
                     , FlexibleContexts
                     , FlexibleInstances
                     , MultiParamTypeClasses
                     , ScopedTypeVariables
                     , TypeFamilies
                     , TypeOperators

  default-language:    Haskell2010
  ghc-options:         -Wall
                       -ddump-simpl-stats

source-repository head
  type:     git
  location: https://github.com/4e6/webapp-template-hs
